{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "agentsib/diadoc-php": "^0.0.3",
        "cocur/slugify": "^4.0",
        "creonit/mailing-bundle": "^1.0",
        "creonit/media-bundle": "^1.0",
        "creonit/nuxt-bundle": "^1.0",
        "creonit/page-bundle": "^1.0",
        "creonit/propel-cache-behavior": "^0.1.0",
        "creonit/propel-schema-converter-bundle": "^0.2.5",
        "creonit/rest-bundle": "^1.0",
        "creonit/storage-bundle": "^1.0",
        "creonit/verification-code-bundle": "^1.0",
        "doctrine/annotations": "^1.0",
        "gregwar/image": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "lcobucci/jwt": "^4.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpoffice/phpspreadsheet": "^1.17",
        "phpro/soap-client": "^1.7",
        "propel/propel": "2.0.x-dev",
        "propel/propel-bundle": "^5.0",
        "sabre/xml": "^2.2",
        "sensio/framework-extra-bundle": "^5.6",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/debug-bundle": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/expression-language": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.1.*",
        "symfony/lock": "5.1.*",
        "symfony/maker-bundle": "^1.21",
        "symfony/mercure-bundle": "^0.2.6",
        "symfony/messenger": "5.1.*",
        "symfony/mime": "5.1.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/property-access": "5.1.*",
        "symfony/property-info": "5.1.*",
        "symfony/security-bundle": "5.1.*",
        "symfony/serializer": "5.1.*",
        "symfony/stopwatch": "5.1.*",
        "symfony/twig-bundle": "5.1.*",
        "symfony/var-dumper": "5.1.*",
        "symfony/web-profiler-bundle": "5.1.*",
        "symfony/yaml": "5.1.*",
        "twig/extra-bundle": "^3.0",
        "twig/twig": "^3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": false
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Propel\\Bundle\\PropelBundle\\": "vendor/propel/propel-bundle/",
            "Gregwar\\ImageBundle\\": "vendor/gregwar/image-bundle/",
            "Creonit\\NuxtBundle\\": "bundles/Creonit/NuxtBundle",
            "Creonit\\UserBundle\\": "bundles/Creonit/UserBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "propel:model:build": "symfony-cmd",
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "propel/propel-bundle",
                "version": "5.0.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/creonit-dev/PropelBundle",
                    "reference": "f8f697b587977ee2512d024bc7b1710bf0e9c450"
                },
                "dist":  {
                    "type": "zip",
                    "url": "https://api.github.com/repos/creonit-dev/PropelBundle/zipball/f8f697b587977ee2512d024bc7b1710bf0e9c450",
                    "reference": "f8f697b587977ee2512d024bc7b1710bf0e9c450"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "gregwar/image-bundle",
                "version": "3.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/creonit-dev/ImageBundle",
                    "reference": "symfony_5_supports"
                }
            }
        }
    ],
    "require-dev": {
        "laminas/laminas-code": "^3.1.0"
    }
}
